{
    "ini_params": [
        {
            "name": "memory_limit",
            "limit": 256
        },
        {
            "name": "max_execution_time",
            "limit": 30
        }
    ],
    "wp_options": [
        {
            "name": "permalink_structure",
            "value": "/%postname%/",
            "gui_name": "[Common Settings] = Post name",
            "path": "/wp-admin/options-permalink.php"
        },
        {
            "name": "woocommerce_calc_taxes",
            "value": "yes",
            "gui_name": "[WooCommerce Settings - General] = Enable taxes",
            "path": "/wp-admin/admin.php?page=wc-settings&tab=general",
            "warn_only": true
        }
    ],
    "mandatory_plugins": [
        "woocommerce/woocommerce.php",
        "rest-api-oauth1/oauth-server.php"
    ],
    "incompatible_plugins": [
        "woocommerce-services/woocommerce-services.php",
